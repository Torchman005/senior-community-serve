<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luminous.senior_community_server.mapper.ManagerMapper">

    <select id="selectByUsername" resultType="com.luminous.senior_community_server.bean.Manager">
        select * from manager where username = #{username}
    </select>

    <insert id="insert" parameterType="com.luminous.senior_community_server.bean.Manager">
        insert into `manager` (username, password) values (#{username}, #{password})
    </insert>

    <update id="updateById" parameterType="com.luminous.senior_community_server.bean.Manager">
        update `manager` set username = #{username}, password = #{password} where id = #{id}
    </update>
</mapper>